pipeline{
    
    agent any
    
    stages{
        
        stage('Build'){
            steps{
                echo'building the pipeline...'
            }
        }
        
        stage('Deploy to QA'){
            steps{
                echo'deploing it to qa.....'
            }
        }
        
        stage('Run Automation Testcases'){
            steps{
                echo'Run Automation Testcases....'
            }
        }
        
        stage('Deploy to Stage'){
            steps{
                echo'building the pipeline...'
            }
        }
        
        stage('Sanity Test'){
            steps{
                echo'Sanity testing...'
            }
        }
        
        stage('Deploy to production'){
            steps{
                echo'deploying to production'
            }
        }
        
    }
}